  "main": "dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/main.js",
    "dev": "ts-node-dev --inspect=0.0.0.0:9229 --poll --respawn --ignore-watch node_modules --transpile-only src/main.ts",
    "debug": "node --inspect=0.0.0.0:9229 --nolazy ./dist/main.js",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "eslint": "npx eslint --ext .ts src",
    "eslint:fix": "npx eslint --ext .ts --fix src",
    "prisma": "prisma",
    "migration:generate": "prisma migrate dev --name",
    "migration:run": "prisma migrate deploy",
    "dev:postgres": "docker-compose -f docker-compose.dev.yml up -d"
  },
